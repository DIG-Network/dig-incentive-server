{
  "name": "dig-cli",
  "version": "1.0.0-alpha.1",
  "description": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "dig": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && npm run postbuild",
    "postbuild": "copyfiles -u 1 \"src/**/*.crt\" \"src/**/*.key\" dist",
    "start": "ts-node src/index.ts",
    "server": "ts-node src/server.ts",
    "test": "mocha -r ts-node/register tests.ts",
    "release": "standard-version",
    "postrelease": "npm run build && git push --follow-tags && npm publish",
    "start-propagation-server": "ts-node src/propagation_server/index.ts",
    "start-content-server": "ts-node src/content_server/index.ts",
    "start-incentive-server": "ts-node src/incentive_server/index.ts",
    "build-content-server": "docker build -t dig-content-server -f Dockerfile.content_server .",
    "build-propagation-server": "docker build -t dig-propagation-server -f Dockerfile.propagation_server .",
    "build-incentive-server": "docker build -t dig-incentive-server -f Dockerfile.incentive_server .",
    "release-propagation-server": "npm run build-propagation-server && docker tag dig-propagation-server michaeltaylor3d/dig-propagation-server:1.0.0-alpha.1 && docker push michaeltaylor3d/dig-propagation-server:1.0.0-alpha.1",
    "release-content-server": "npm run build-content-server && docker tag dig-content-server michaeltaylor3d/dig-content-server:1.0.0-alpha.1 && docker push michaeltaylor3d/dig-content-server:1.0.0-alpha.1",
    "release-incentive-server": "npm run build-incentive-server && docker tag dig-incentive-server michaeltaylor3d/dig-incentive-server:1.0.0-alpha.1 && docker push michaeltaylor3d/dig-incentive-server:1.0.0-alpha.1",
    "release-docker": "npm run release-propagation-server && npm run release-content-server && npm run release-incentive-server"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "async-mutex": "^0.5.0",
    "bip39": "^3.1.0",
    "chia-bls": "^1.0.2",
    "chia-config-loader": "^1.0.1",
    "chia-root-resolver": "^1.0.0",
    "chia-server-coin": "^0.0.5",
    "chia-wallet": "^1.0.18",
    "cli-progress": "^3.12.0",
    "crypto-js": "^4.2.0",
    "datalayer-driver": "^0.1.21",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "ignore": "^5.3.2",
    "inquirer": "^10.1.8",
    "ipify": "^6.0.0",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.4.0",
    "nanospinner": "^1.1.0",
    "nconf": "^0.12.1",
    "public-ip": "^7.0.1",
    "superagent": "^10.0.0",
    "toad-scheduler": "^3.0.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.17",
    "@types/cli-progress": "^3.11.6",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/keytar": "^4.4.2",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.7",
    "@types/nconf": "^0.10.7",
    "@types/node": "^22.1.0",
    "@types/superagent": "^8.1.8",
    "@types/yargs": "^17.0.33",
    "chai": "^5.1.1",
    "copyfiles": "^2.4.1",
    "mocha": "^10.7.0",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
